<%= form_for @resource do |f| %>

  <p>title: <%= f.text_field :title %></p>
  <p>description: <%= f.text_area :description %></p>
  <p>media_type: <%= f.text_field :media_type %></p>
  <p>url: <%= f.text_field :url %></p>
  <p>file_upload: <%= f.file_field :featured_image %></p>

  <%= f.submit%>
<% end %>
